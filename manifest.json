{
    "name": "WhatsTap",
    "description": "Google Chrome extension to open a mobile number in WhatsApp Web",
    "version": "1.0",
    "manifest_version": 3,
    "icons": {
        "16": "icon.png",
        "48": "icon.png",
        "128": "icon.png"
    },
    "permissions": [
        "contextMenus",
        "storage",
        "notifications"
    ],
    "background": {
        "service_worker": "content.js"
    }
}